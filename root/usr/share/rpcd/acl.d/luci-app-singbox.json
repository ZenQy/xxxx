{
	"luci-app-singbox": {
		"description": "Grant access to LuCI app sing-box",
		"read": {
			"file": {
				"/etc/passwd": [
					"read"
				],
				"/etc/group": [
					"read"
				]
			},
			"ubus": {
				"service": [
					"list"
				]
			},
			"uci": [
				"singbox"
			]
		},
		"write": {
			"uci": [
				"singbox"
			]
		}
	}
}